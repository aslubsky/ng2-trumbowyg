{
    "name": "ng2-trumbowyg",
    "version": "1.0.0",
    "description": "Angular2 bind for trumbowyg",
    "scripts": {
        "compile-bootstrap": "node-sass --output-style=compressed ./modified-bootstrap.scss ./assets/modified-bootstrap.css && cp -r ./node_modules/font-awesome/fonts ./assets/fonts",
        "ng": "ng",
        "build": "ng build ng2-trumbowyg --prod"
    },
    "main": "ng2-trumbowyg.js",
    "typings": "ng2-trumbowyg.d.ts",
    "keywords": [
        "angular2",
        "trumbowyg"
    ],
    "author": "aslubsky@gmail.com",
    "license": "MIT",
    "dependencies": {},
    "peerDependencies": {
        "trumbowyg": "git://github.com/davintoo/Trumbowyg.git#master"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.1.2",
        "@angular-devkit/build-ng-packagr": "^0.1002.0",
        "@angular/cli": "^12.1.2",
        "@angular/common": "^12.1.2",
        "@angular/compiler": "^12.1.2",
        "@angular/compiler-cli": "^12.1.2",
        "@angular/core": "^12.1.2",
        "@angular/forms": "^12.1.2",
        "@angular/platform-server": "^12.1.2",
        "font-awesome": "^4.7.0",
        "ng-packagr": "^10.0.4",
        "rxjs": "~6.6.7",
        "typescript": "~4.2.4"
    }
}
